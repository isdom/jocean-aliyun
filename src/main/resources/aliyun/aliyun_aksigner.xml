<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean id="aksigner" class="org.jocean.aliyun.sign.AKSigner"/>
    <bean id="aksigner2" class="org.jocean.aliyun.sign.AKSigner2"/>
    
    <alias name="aksigner" alias="${signer.name:aksigner.default}"/>
    <alias name="aksigner2" alias="${signer2.name:aksigner2.default}"/>
</beans>
